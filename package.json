{
  "name": "craft-pokemon",
  "version": "0.0.1",
  "description": "Craft Demo Designs - Pokemon San Diego",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donsuhr/craft-pokemon.git"
  },
  "bugs": {
    "url": "https://github.com/donsuhr/craft-pokemon/issues"
  },
  "homepage": "https://github.com/donsuhr/craft-pokemon#readme",
  "author": {
    "name": "Donald Suhr",
    "email": "github@donsuhr.com",
    "url": "http://www.donsuhr.com/"
  },
  "scripts": {
    "build": "rm -rf ./dist && webpack",
    "dist": "rm -rf ./dist && NODE_ENV=production webpack",
    "check-types": "tsc --noEmit",
    "esw": "esw --watch --ext .js,.jsx,.ts,.tsx --color --cache",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:styles": "stylelint \"./src/**/*.scss\" --cache ",
    "pre-commit": "npm run check-types && npm run test:coverage && npm run lint && npm run lint:styles",
    "start": "rm -rf ./dist && webpack serve",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "watch:check-types": "tsc --noEmit --watch --preserveWatchOutput",
    "watch": "concurrently \"npm run watch:check-types\" \"npm run esw\" \"node bin/watch-stylelint.js\" --raw",
    "wba": "NODE_ENV=production webpack --progress --analyze"
  },
  "jest": {
    "coverageReporters": [
      "json",
      "html"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^@/(.*)": "<rootDir>/src/$1"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFilesAfterEnv": [
      "./jest-setup.ts"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@types/classnames": "^2.2.11",
    "@types/googlemaps": "^3.40.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-test-renderer": "^16.9.3",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "chokidar": "^3.4.3",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.3.0",
    "css-loader": "^5.0.1",
