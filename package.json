{
  "name": "craft-pokemon",
  "version": "0.0.1",
  "description": "Craft Demo Designs - Pokemon San Diego",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donsuhr/craft-pokemon.git"
  },
  "bugs": {
    "url": "https://github.com/donsuhr/craft-pokemon/issues"
  },
  "homepage": "https://github.com/donsuhr/craft-pokemon#readme",
  "author": {
    "name": "Donald Suhr",
    "email": "github@donsuhr.com",
    "url": "http://www.donsuhr.com/"
  },
  "scripts": {
    "build": "webpack",
    "check-types": "tsc --noEmit",
    "esw": "esw --watch --ext .js,.jsx,.ts,.tsx --color --cache",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:styles": "stylelint \"./src/**/*.scss\" --cache ",
    "pre-commit": "npm run check-types && npm run test:coverage && npm run lint && npm run lint:styles",
    "start": "webpack serve --hot",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "watch:check-types": "tsc --noEmit --watch --preserveWatchOutput",
    "watch": "concurrently \"npm run watch:check-types\" \"npm run esw\" \"node bin/watch-stylelint.js\" --raw"
  },
  "jest": {
    "coverageReporters": [
      "json",
      "html"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "chokidar": "^3.4.3",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.0",
    "css-modules-typescript-loader": "^4.0.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-watch": "^7.0.0",
    "fibers": "^5.0.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.1",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.2.1",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.1",
    "sass": "^1.28.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-use-variable": "^1.7.2",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "typescript": "^4.0.5",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  }
}
